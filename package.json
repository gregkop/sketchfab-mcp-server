{
  "name": "sketchfab-mcp",
  "version": "1.0.0",
  "description": "A Model Context Protocol (MCP) server for interacting with Sketchfab",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sketchfab-mcp": "dist/index.js",
    "sketchfab-mcp-server": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
