  },
  "devDependencies": {
    "@types/node": "^22.13.5",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
